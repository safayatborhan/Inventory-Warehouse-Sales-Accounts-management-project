

<div class="row">
    <label>Local Payment(পেমেন্ট)</label>
    <div class="col-md-12">
        <div class="card">
            <div class="row">
                <div class="col-lg-12">
                    <div class="well bs-component">
                        <form class="form-horizontal">
                            <fieldset>
                                <div class="form-group">
                                    <label class="col-md-1 control-label">
                                       From Date:
                                    </label>
                                    <div class="col-md-3">
                                        <input type="date" id="txtToDate" name="txtDate" class="form-control" />
                                    </div>
                                    <label class="col-md-1 control-label">
                                       To Date:
                                    </label>
                                    <div class="col-md-3">
                                        <input type="date" id="txtFromDate" name="txtDate" class="form-control" />
                                    </div>
                                </div>
                                <div class="ibox-content" id="divInfo">
                                    <div class="form-group">
                                        <div class="col-md-12 table-responsive" id="div-productGroup">
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                    <div>
                        <table id="PaymentReportTableModal" class="table order-list">
                            <thead>
                                <tr>
                                    <td>Date</td>
                                    <td>Party</td>
                                    <td>Invoice No</td>
                                    <td>Total Amount</td>
                                    <td>Paid Amount</td>
                                    <td>Rest Amount</td>
                                    <td>Payment Cash</td>
                                    <td>Payment Checque</td>
                                    <td>Check No</td>
                                    <td>Check Date</td>
                                    <td>Bank</td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="col-sm-2">
                                        <label type="date"  class="form-inline">25-10-2017</label>
                                    </td>
                                    <td class="col-sm-2">
                                        <a href="/Payment/PaymentHistory">Party1</a>
                                    </td>
                                    <td class="col-sm-1">
                                        <a href="/Payment/PaymentHistory">INV0001</a>
                                    <td class="col-sm-1">
                                        <label type="date" class="form-inline">1000</label>
                                    </td>
                                    <td class="col-sm-1">
                                        <a href="/Payment/PaymentHistory">500</a>
                                    </td>
                                    <td class="col-sm-1">
                                        <a href="/Payment/PaymentHistory">500</a>
                                    </td>
                                    <td class="col-sm-2">
                                        <input type="text" name="txtPayment" class="form-control" />
                                    </td>
                                    <td class="col-sm-3">
                                        <input type="text" name="txtCheckNo" class="form-control" />
                                    </td>
                                    <td class="col-sm-2">
                                        <input type="date" name="txtCheckDate" class="form-control" />
                                    </td>
                                    <td class="col-sm-3">
                                        <select id="ddlAccountLedger" class="form-control">
                                            <option value="0">পছন্দ করুন</option>
                                            <option value="1">AB Bank</option>
                                            <option value="1">NBL</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <td class="col-sm-1">
                                        <label type="date" class="form-inline">25-10-2017</label>
                                    </td>
                                    <td class="col-sm-2">
                                        <a href="/Payment/PaymentHistory">Party2</a>
                                    </td>
                                    <td class="col-sm-1">
                                        <a href="/Payment/PaymentHistory">INV000001</a>
                                    <td class="col-sm-1">
                                        <label type="date" class="form-inline">1000000</label>
                                    </td>
                                    <td class="col-sm-1">
                                        <a href="/Payment/PaymentHistory">500000</a>
                                    </td>
                                    <td class="col-sm-1">
                                        <a href="/Payment/PaymentHistory">500000</a>
                                    </td>
                                    <td class="col-sm-2">
                                        <input type="text" name="txtPayment" class="form-control" />
                                    </td>
                                    <td class="col-sm-2">
                                        <input type="text" name="txtPaymentCheck" class="form-control" />
                                    </td>
                                    <td class="col-sm-3">
                                        <input type="text" name="txtCheckNo" class="form-control" />
                                    </td>
                                    <td class="col-sm-2">
                                        <input type="date" name="txtCheckDate" class="form-control" />
                                    </td>
                                    <td class="col-sm-3">
                                        <select id="ddlAccountLedger" class="form-control">
                                            <option value="0">পছন্দ করুন</option>
                                            <option value="1">AB Bank</option>
                                            <option value="1">NBL</option>
                                        </select>
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot>
                                <tr></tr>
                                <tr></tr>
                            </tfoot>
                        </table>

                        <div class="ibox-content" id="divInfo">
                            <div class="form-group">
                                <div class="col-md-12 table-responsive" id="div-product-add">
                                    <script id="templateProductGroupModal" type="text/html">
                                        <table class="table table-bordered table-striped table-hover" id="productGroupTableModal">
                                            <thead>
                                            <th>ID</th>
                                            <th>Name (ক্রেতার নাম)</th>
                                            <th>Phone (মোবাইল)</th>
                                            <th>Email (ইমেইল)</th>
                                            <th>Action</th>
                                            </thead>
                                            <tbody>
                                                {{#ProductGroupSearch}}

                                                <tr>
                                                    <td> {{Id}}</td>
                                                    <td> {{Name}}</td>
                                                    <td>{{Phone}}</td>
                                                    <td>{{Email}}</td>
                                                    <td class="action-sm">
                                                        <a class="btn btn-primary btn-xs" onclick="LoadForEdit('{{Id}}')"><i class="fa fa-edit"></i> Select</a>
                                                    </td>
                                                </tr>

                                                {{/ProductGroupSearch}}
                                            </tbody>
                                        </table>
                                    </script>
                                </div>
                            </div>
                        </div>
                    </div>
                    <fieldset>
                        <div class="form-actions">
                            <div class="col-md-12">
                                <button class="btn btn-w-m btn-info" data-style="expand-left" id="btnSave" type="button" onclick="Save()"><i class="fa fa-save"></i>&nbsp; Save</button>
                                <button class="btn btn-w-m btn-success" type="button" id="btnReset" onclick="ResetForm()"><i class="fa fa-refresh"></i>&nbsp;Reset</button>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/app/CommonLoader.js"></script>
<script src="~/Scripts/app/LocalPayment.js"></script>


